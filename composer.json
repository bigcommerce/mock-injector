{
    "name": "bigcommerce/mock-injector",
    "type": "library",
    "description": "Auto-mocking injector for testing DI components",
    "keywords": [
        "di",
        "pimple",
        "injector",
        "test"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Benson",
            "email": "adam.benson@bigcommerce.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3 || ^8.0",
        "bigcommerce/injector": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bigcommerce\\MockInjector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
